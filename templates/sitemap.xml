<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
$for(posts)$<url>
      <loc>$baseURL$$url$</loc>
        <changefreq>weekly</changefreq>
        $if(dateMap)$<lastmod>$dateMap$</lastmod>$endif$
        <priority>0.8</priority>
    </url>$endfor$
</urlset>
